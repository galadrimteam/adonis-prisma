{
  "name": "@galadrim/adonis-prisma",
  "description": "Prisma provider for AdonisJS",
  "version": "0.0.10",
  "engines": {
    "node": ">=20.6.0"
  },
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests",
    "!build/test-helpers"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./prisma_provider": "./build/providers/prisma_provider.js",
    "./commands": "./build/commands/main.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "pnpm run lint",
    "test": "c8 pnpm run quick:test",
    "prebuild": "pnpm run lint && pnpm run clean",
    "build": "tsc",
    "postbuild": "pnpm run copy:templates && pnpm run index:commands",
    "release": "np",
    "version": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "index:commands": "adonis-kit index build/commands"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/auth": "^9.3.1",
    "@adonisjs/core": "6.17.0",
    "@adonisjs/eslint-config": "2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^3.0.0",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^3.1.4",
    "@poppinss/utils": "^6.9.2",
    "@prisma/client": "^6.2.1",
    "@swc/core": "^1.6.3",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.15.0",
    "execa": "^9.5.2",
    "np": "^10.0.6",
    "pgmock": "^1.0.3",
    "prettier": "^3.3.2",
    "prisma": "^6.2.1",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@adonisjs/auth": "^9.3.1",
    "@adonisjs/core": "^6.17.0",
    "@prisma/client": "^6.2.1"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
